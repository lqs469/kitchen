{
  "name": "origin",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "dependencies": {
    "egg-scripts": "^2.13.0",
    "egg-view-assets": "^1.6.0",
    "egg-view-nunjucks": "^2.2.0",
    "midway": "^1.18.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.5.5",
    "cross-env": "^6.0.0",
    "egg-ci": "^1.13.1",
    "midway-bin": "^2.0.0-beta.14",
    "midway-mock": "^1.18.0",
    "ts-node": "^8.3.0",
    "tslib": "^1.8.1",
    "tslint": "^5.11.0",
    "tslint-midway-contrib": "1",
    "typescript": "^3.5.0"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "scripts": {
    "postinstall": "cd client/index && npm install",
    "dev:client": "cd client/index && npm run start",
    "dev": "cross-env NODE_ENV=local midway-bin dev --ts",
    "start": "egg-scripts start --daemon --title=midway-server-kitchen --framework=midway --ts",
    "stop": "egg-scripts stop --title=midway-server-kitchen",
    "start_build": "npm run build && cross-env NODE_ENV=local midway-bin dev",
    "clean": "midway-bin clean",
    "debug": "cross-env NODE_ENV=local midway-bin debug --ts",
    "test": "npm run lint & midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "tslint -c tslint.json --project .",
    "ci": "npm run cov",
    "build": "npm run server_build && npm run client_build",
    "client_build": "cd client/index && npm run build && cp -r ./build ../../dist/app/public && cp ./build/manifest.json ../../dist/config",
    "server_build": "midway-bin build -c",
    "lint-fix": "npm run lint -- --fix"
  },
  "ci": {
    "version": "10, 12"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT",
  "midway-bin-build": {
    "include": [
      "app/public",
      "app/view",
      "config/manifest.json"
    ]
  }
}
